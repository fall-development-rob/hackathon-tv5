{
  "name": "@media-gateway/arw",
  "version": "0.1.0",
  "description": "Agent-Ready Web (ARW) specification implementation for AI agent integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./manifest": {
      "types": "./dist/manifest/index.d.ts",
      "import": "./dist/manifest/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    }
  },
  "dependencies": {
    "@media-gateway/core": "workspace:*",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "arw",
    "agent-ready-web",
    "ai-agents",
    "machine-views",
    "semantic-actions"
  ],
  "license": "Apache-2.0"
}
